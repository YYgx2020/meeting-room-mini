.room-list-page .header-box {
  width: 100vw;
  height: 112rpx;
  background-color: var(--bgColor1);
  display: flex;
  line-height: 112rpx;
  justify-content: space-between;
  font-size: 36rpx;
}
.room-list-page .header-box .header-box__left {
  padding-left: 32rpx;
  align-self: flex-start;
  width: 30%;
  box-sizing: border-box;
}
.room-list-page .header-box .header-box__middle {
  align-self: center;
  width: 40%;
  text-align: center;
}
.room-list-page .header-box .header-box__right {
  align-self: flex-end;
  padding-right: 32rpx;
  width: 30%;
  box-sizing: border-box;
  text-align: right;
}
.room-list-page .header-box .header-box__right text {
  font-size: 42rpx;
  padding-left: 24rpx;
}
.room-list-page .scroll-box {
  width: 100vw;
  height: calc(100vh - 112rpx);
  background-color: var(--bgColor1);
  display: flex;
  flex-direction: column;
  align-items: center;
}
.room-list-page .scroll-box .meeting-room-item {
  width: calc(100vw - 64rpx);
  background-color: var(--bgColor2);
  padding-bottom: 32rpx;
  margin-top: 32rpx;
  border-radius: 12rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}
.room-list-page .scroll-box .meeting-room-item .room-cover {
  width: 100%;
  height: 300rpx;
}
.room-list-page .scroll-box .meeting-room-item .room-cover image {
  border-top-left-radius: 12rpx;
  border-top-right-radius: 12rpx;
  width: 100%;
  height: 100%;
}
.room-list-page .scroll-box .meeting-room-item .item-content {
  padding-top: 16rpx;
  padding-left: 32rpx;
  display: flex;
  flex-direction: column;
}
.room-list-page .scroll-box .meeting-room-item .item-content .label {
  line-height: 64rpx;
  display: flex;
  align-items: center;
}
.room-list-page .scroll-box .meeting-room-item .item-content .label .label-icon {
  color: #595959;
}
.room-list-page .scroll-box .meeting-room-item .item-content .label text {
  padding-left: 8rpx;
  font-size: 36rpx;
}
